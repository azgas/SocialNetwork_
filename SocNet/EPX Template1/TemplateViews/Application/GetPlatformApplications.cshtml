@using EPX_Template1.TemplateModels.Application

@model List<Application>

@if (Model.Count == 0)
{
    <a href="">@EPX_Template1.TemplateResources.Application.GetPlatformApplications.NoApplicationsAvailable</a>
}

@foreach (var applicaion in Model)
{
    <a href="@applicaion.Url">@applicaion.Name</a>
}